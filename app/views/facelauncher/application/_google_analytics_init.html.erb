<%-# Include this at the bottom of the head. %>
<%- if !@program.program_app.nil? and !@program.program_app.google_analytics_tracking_code.blank? %>
  <script type="text/javascript">
    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', '<%= @program.program_app.google_analytics_tracking_code %>']);
    <%-# The following lines are needed if this page is in a Facebook iframe app. %>
    <%- if @program.program_app.app_url =~ /https?:\/\/www\.facebook\.com\// %>
    _gaq.push(['_addIgnoredRef', 'static.ak.facebook.com']);
    _gaq.push(['_addIgnoredRef', 's-static.ak.facebook.com']);
    <%- end %>
    _gaq.push(['_trackPageview']);

    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();
  </script>
<%- end %>